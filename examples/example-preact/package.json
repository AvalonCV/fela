    "fela-perf": "^12.0.0-rc.0",
    "fela-plugin-embedded": "^12.0.0-rc.0",
    "fela-plugin-fallback-value": "^12.0.0-rc.0",
    "fela-plugin-logger": "^12.0.0-rc.0",
    "fela-plugin-lvha": "^5.0.16",
    "fela-plugin-prefixer": "^12.0.0-rc.0",
    "fela-plugin-unit": "^12.0.0-rc.0",
